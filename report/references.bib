@inproceedings{realistic,
    abstract = {{Accurately simulating user movements in mobile ad hoc networks (MANETs) is very important to the prediction of actual network and user performance. Therefore, using a realistic mobility model is a crucial factor in enhancing the confidence in the simulation results of these networks. In this paper, we present a new mobility model for MANETs, called realistic mobility model (RMM). In RMM, node velocities and directions of movement are taken from probability distributions that mimic real user mobility behavior. The model is shown to be stationary since it satisfies the conditions of (J.Y. Lee et al., 2005). Simulation results show that RMM produces mobility traces that closely resemble real mobility traces.}},
    author = {Kamal, A. E. and Al-Karaki, J. N.},
    booktitle = {2007 IEEE International Conference on Communications},
    citeulike-article-id = {7360478},
    citeulike-linkout-0 = {http://dx.doi.org/10.1109/ICC.2007.558},
