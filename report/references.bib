    pages = {123--131},
    posted-at = {2009-03-19 03:54:05},
    priority = {2},
    title = {{Mesh networks: commodity multihop ad hoc networks}},
    url = {http://dx.doi.org/10.1109/MCOM.2005.1404606},
    volume = {43},
    year = {2005}
}

